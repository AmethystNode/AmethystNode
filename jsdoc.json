{
    "plugins": [],
    "recurseDepth": 10,
    "source": {
        "exclude": ["./node_modules", "./src/amethystnode/entity/Entity.js", "./src/amethystnode/nbt/NBT.js"],
        "includePattern": ".+\\.js(doc|x)?$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "sourceType": "module",
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc","closure"]
    },
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false
    }
}
